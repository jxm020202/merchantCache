# Google Custom Search API Credentials
# SECURITY: Never commit this file with real values to git!
# Copy to .env and add your actual credentials

# API Key (from Google Cloud Console)
GOOGLE_API_KEY=your_api_key_here

# Search Engine ID (from Programmable Search Engine)
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# OAuth2 Client ID (from Google Cloud Console OAuth app)
GOOGLE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com

# OAuth2 Client Secret (KEEP PRIVATE - Never share or commit)
GOOGLE_CLIENT_SECRET=your_client_secret_here

# OAuth2 Redirect URI (must match registered URI in Google Console)
GOOGLE_REDIRECT_URI=http://localhost:8080/callback
